        default { "off" }
        hidewhen "{ addprefix == 0 }"
        range   { 0 10 }
        parmtag { "autoscope" "0000000000000000" }
        parmtag { "script_callback_language" "python" }
    }
    parm {
        name    "separator"
        label   "separator"
        type    string
        default { "_" }
        parmtag { "autoscope" "0000000000000000" }
        parmtag { "script_callback_language" "python" }
    }

