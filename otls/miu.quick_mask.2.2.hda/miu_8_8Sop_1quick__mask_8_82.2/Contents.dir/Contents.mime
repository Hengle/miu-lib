//SWITCH TYPE
int size = detail(0, \"___attribSizeIn\"); //attribsize(0, \"point\", attr);
float val_0, val_1, val_2, val_3 = -1; // create placeholder array for vector4
float temp[] = {0, 0, 0, 0};
if(size==1)
{
    float test = point(0, attr, @ptnum);
    assign(val_0, test);
    temp = array(val_0);
}

else if(size==2)
{
    vector2 test = point(0, attr, @ptnum);
    assign(val_0, val_1, test);
    temp = array(val_0, val_1);
}

else if(size==3)
{
    vector test = point(0, attr, @ptnum);
    assign(val_0, val_1, val_2, test);
    temp = array(val_0, val_1, val_2);
}

else if(size==4)
{
    vector4 test = point(0, attr, @ptnum);
    assign(val_0, val_1, val_2, val_3, test);
    temp = array(val_0, val_1, val_2, val_3);
}

else{
    sprintf(\"This type is incompatible, supported types are float, vector2, vector and vector4\");
}

//SET TEMPORARY ARRAY VARIABLE
//float temp[] = array(val_0, val_1, val_2, val_3);
float val = temp[component];
//---------------------------------

//SHAPING ATTRIBUTE
float freq = chf(\"freq\");
float offset = chf(\"offset\");
float center = chf(\"center\");
float width = chf(\"width\");
float imin = chf(\"inmin\");
float imax = chf(\"inmax\");
float omin = chf(\"onmin\");
