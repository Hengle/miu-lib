float ipow = chf(\"inputPow\");
float opow = chf(\"outPow\");

`ifs(ch(\"../do_vexpression\"), chs(\"../vexpression\"), \"\")`

width *= chf(\"inmax\") - chf(\"inmin\");
center = efit(center, 0, 1, chf(\"inmin\")-width, chf(\"inmax\")+width);

float widthMin = center - width;
float widthMax = center + width;

float inrange = fit(val, widthMin, widthMax,0,1); // set input range
inrange = pow(inrange, ipow); // pre pow
inrange *= freq;
inrange += offset;

if(chi(\"mirror\")){
    float mirrorRamp = fit(inrange, 0, 1, 1, 0);
    inrange *= mirrorRamp;
    inrange *= 4;
}
float ramprange = chramp(\"inputAttributeRamp\", inrange); //ramp

ramprange = pow(ramprange, opow); // post pow
float outrange = fit01(ramprange, omin, omax); // out range
outrange = clamp(outrange, omin, omax);
//---------------------------------

// //OUTPUT ATTRIBUTE
f@___val = outrange;"	)
