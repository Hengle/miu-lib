Content-Disposition: attachment; filename="hdaroot/normalize_input_attrib/float_type.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 8	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"float clamp = floor(detail(0, \"___min\") - detail(0, \"___max\"));
float offset = set(0);
clamp = ceil(abs(detail(0, \"___min\") - detail(0, \"___max\")));
clamp = clamp(clamp, 0, 1);
offset = fit(clamp, 0, 1, 1, 0);
offset *= detail(0, \"___min\");

float min = detail(0, \"___min\");
float max = detail(0, \"___max\");

float outMin = set(0);
float outMax = set(1);

string attrName = detail(0, \"___attribNameIn\");
string prefix = detail(0, \"___prefix\");
string attrNameOut = attrName; 

string seps[] =  detail(0, \"___seps\");
int hasSep = detail(0, \"___addSep\");

if(chi(\"../../normalize\")){
    
    //detect type
    if(detail(0, \"___classIndex\") == 0)
    {
        float val = detail(0, attrName, 1);
        val = fit(val, min, max, outMin, outMax);
        val *= clamp;
        val += offset;
        setdetailattrib(0, attrNameOut, val);
    }
    
    else if(detail(0, \"___classIndex\") == 1)
    {
        float val = prim(0, attrName, @primnum);
        val = fit(val, min, max, outMin, outMax);
        val *= clamp;
        val += offset;
        setprimattrib(0, attrNameOut, @primnum, val);
    }
    
    else if(detail(0, \"___classIndex\") == 2)
    {
        float val = point(0, attrName, @ptnum);
        val = fit(val, min, max, outMin, outMax);
        val *= clamp;
        val += offset;
        setpointattrib(0, attrNameOut, @ptnum, val);
    }
    
    else if(detail(0, \"___classIndex\") == 3)
    {
        float val = vertex(0, attrName, @primnum, @vtxnum);
        val = fit(val, min, max, outMin, outMax);
        val *= clamp;
        val += offset;
        setvertexattrib(0, attrNameOut, @primnum, @vtxnum, val);
    }
}else{
    //detect type
    if(detail(0, \"___classIndex\") == 0)
    {
        float val = detail(0, attrName, 1);
        setdetailattrib(0, attrNameOut, val);
    }
    
    else if(detail(0, \"___classIndex\") == 1)
    {
        float val = prim(0, attrName, @primnum);
        setprimattrib(0, attrNameOut, @primnum, val);
    }
    
    else if(detail(0, \"___classIndex\") == 2)
    {
        float val = point(0, attrName, @ptnum);
        setpointattrib(0, attrNameOut, @ptnum, val);
    }
    
    else if(detail(0, \"___classIndex\") == 3)
    {
        float val = vertex(0, attrName, @primnum, @vtxnum);
        setvertexattrib(0, attrNameOut, @primnum, @vtxnum, val);
    }
}"	)
